 #include "Settings.h"

 static const SchedulingPolicy policies[] = {
-   [SCHED_OTHER] = { "Other", SCHED_OTHER, false },
-   [SCHED_BATCH] = { "Batch", SCHED_BATCH, false },
-   [SCHED_IDLE] = { "Idle", SCHED_IDLE, false },
