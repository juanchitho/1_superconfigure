+   { "Batch", SCHED_BATCH, false },
+   { "Idle", SCHED_IDLE, false }
 };

 const SchedulingPolicy *getSchedulingPolicy(int policy) {
+   switch (policy) {
+       case SCHED_OTHER: return &policies[0];
+       case SCHED_FIFO: return &policies[1];
+       case SCHED_RR: return &policies[2];
+       case SCHED_BATCH: return &policies[3];
+       case SCHED_IDLE: return &policies[4];
+   }
     return NULL;
 }
